RoundBox {
    border: 2px solid darkgray;    
    border-radius: 5px;
    background-color: cyan;
}


CircleWidget {
    border: 2px solid gray;
    border-radius: 3px;
    background-color: cyan;
    color: gray;
    padding: 3px;
}

CircleWidget:hover {
    background-color: #45a049;
}

PlateWidget {
    background-color: #DDDDE1;
    border: 4px solid darkGray;
    border-radius: 20px; /* Adjust radius as needed */
}
